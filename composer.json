{
    "name": "kaabar-sso/yii2-sso",
    "description": "Connect Yii 2 application to a SAML Identity Provider for Single Sign On",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "saml",
        "single sign on"
    ],
    "license": "MIT",
    "authors": [{
            "name": "Chetan Patel",
            "email": "cpjeslot@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2": "^2.0.48",
        "onelogin/php-saml": "^3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.34",
        "doctrine/instantiator": "1.0.5"
    },
    "autoload": {
        "psr-4": {
            "kaabar\\sso\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "kaabar\\sso\\tests\\": "tests/"
        }
    }
}